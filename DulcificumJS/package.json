{
  "name": "@ultimaker/dulcificumjs",
  "version": "0.1.0-alpha.1",
  "description": "DulcificumJS is a TS component to run Dulcificum in a browser",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "lint:tsc": "tsc",
    "build": "tsc -p tsconfig.json",
    "copy:build": "cp ../build/Release/DulcificumJS/cpp/dulcificum_js.* src/Dulcificum/",
    "copy:dist": "mkdir -p dist/Dulcificum && cp src/Dulcificum/dulcificum_js.* dist/Dulcificum/",
    "prepublish": "npm run copy:build && npm run build && npm run copy:dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ultimaker/synsepalum-dulcificum.git"
  },
  "keywords": [
    "Cura",
    "Dulcificum",
    "Configuration"
  ],
  "author": "UltiMaker",
  "license": "",
  "bugs": {
    "url": "https://github.com/Ultimaker/synsepalum-dulcificum/issues"
  },
  "homepage": "https://github.com/Ultimaker/synsepalum-dulcificum#readme",
  "dependencies": {
    "emscripten": "^0.0.2-beta"
  },
  "devDependencies": {
    "@types/emscripten": "^1.39.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ]
}
